{
  "expo": {
    "name": "Cerverica-movil-CRM-RN",
    "slug": "Cerverica-movil-CRM-RN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono_completo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icono_completo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5d9ab"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icono_completo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.CervericamovilCRMRN"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicono_navbar.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
